@{
    ViewData["Title"] = "New to Old Tenants";
}
@section Scripts {
    <script src="~/lib/fusioncharts/js/fusioncharts.js"></script>
    <script src="~/lib/fusioncharts/js/themes/fusioncharts.theme.fint.js"></script>
    <script src="~/js/FusionChartScripts.js"></script>
}
<h2>@ViewData["Title"].</h2>
 <!-- Sidebar -->
<div id="sidebar-wrapper">
    <ul class="sidebar-nav">
        <li class="sidebar-brand">
            <a href="#">
                Options
            </a>
        </li>
        <li>
            <div class="form-group">
                <label>Select a month:</label>
                <input type="text" class="form-control form-control-1 input-sm monthPicker" placeholder="MM/YYYY">
            </div>
        </li>
    </ul>
</div>
<!-- /#sidebar-wrapper -->
<div class="row">
    <div class="col-md-offset-2 col-md-7">
        <div id="chartContainer">FusionCharts XT will load here!</div>
    </div>
</div>
<div class="row">
    <footer class="col-md-12">
        <hr />
        <p class="text-center">&copy; 2016 - ChartingProject</p>
    </footer>
</div>

